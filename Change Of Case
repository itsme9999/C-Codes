#include<stdio.h>
int main()
{
  char ch;
  printf("Enter a character to change a case:");
  scanf("%c",&ch);
  printf("%d",ch);
  
  if(ch>=65&&ch<=90)
  {
  	ch=ch+32;
  	printf("\nCharacter after change in case :%c",ch);
  }
  else if(ch>=97&&ch<=112)
  {
  	ch=ch+32;
  	printf("\nCharacter after change in case :%c",ch);
  }
  return 0;
  }
