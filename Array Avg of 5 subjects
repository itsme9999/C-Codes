#include<stdio.h>
#include<conio.h>

int main()
{
    int arr[5],sum=0,i;
    printf("Enter the marks of 5 Subjects:\n");
    for(i=0;i<5;i++)
    {
    scanf("%d",&arr[i]);
    }
     for(i=0;i<5;i++)
     {
        sum=sum+arr[i];
     }
     float avg=sum/5;
     printf("Average:%f",avg);
     printf("Sum of Array:%d",sum);
           
     
 }
